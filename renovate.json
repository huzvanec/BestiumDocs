{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "baseBranchPatterns": [
    "dev"
  ],
  "automerge": true,
  "ignoreTests": true,
  "packageRules": [
    {
      "matchBaseBranches": [
        "dev"
      ],
      "matchUpdateTypes": [
        "minor",
        "patch",
        "pin",
        "digest"
      ]
    }
  ]
}
